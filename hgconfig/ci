changeset         = ci.template
changeset_verbose = ci.verbose.template

file_mod  = '  M {file_mod}\n'
file_add  = '  + {file_add}\n'
file_del  = '  - {file_del}\n'
file_copy = ' {name} ({source})'

start_branches  = ' [1;32m['
branch          = '{branch},'
last_branch     = '{branch}][0m'

start_tags      = ' [0;32m'
tag             = '{tag} '
last_tag        = '{tag}[0m'

start_bookmarks = ' [0;33m('
bookmark        = '{bookmark},'
last_bookmark   = '{bookmark})[0m'



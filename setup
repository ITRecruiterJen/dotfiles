
rm -rf ~/.bashrc
rm -rf ~/.gitconfig
rm -rf ~/.vim
rm -rf ~/.vimrc
rm -rf ~/.irbrc

ln -s ~/.dotfiles/bashrc    ~/.bashrc
ln -s ~/.dotfiles/gitconfig ~/.gitconfig
ln -s ~/.dotfiles/vim       ~/.vim
ln -s ~/.dotfiles/vimrc     ~/.vimrc
ln -s ~/.dotfiles/irbrc     ~/.irbrc

git -C ~/.dotfiles config --local user.name "Jesse James"
git -C ~/.dotfiles config --local user.email "jjames@workwithopal.com" 
